{
  "name": "phoenix-visualization-platform",
  "version": "2.0.0",
  "description": "零故障微服务可视化平台",
  "main": "assets/js/phoenix-visualization.js",
  "scripts": {
    "migrate": "node tools/phoenix-migrate.js",
    "migrate:verbose": "node tools/phoenix-migrate.js --verbose",
    "migrate:rollback": "node tools/phoenix-migrate.js --rollback",
